<%= form_for(:session, url: login_path) do |f| %>

  <div class="field">
    <%= f.label :email %>:<br>
    <%= f.text_field :email %>
  </div>

  <div class="field">
  <%= f.label :password %>
  <%= f.password_field :password %>

  <div class="actions">
    <%= f.submit "Sign In"  %>
  </div>
<% end %>