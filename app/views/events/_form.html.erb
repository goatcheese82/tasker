<%= form_for @event do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :date %>
    <%= f.datetime_select :date %>
    <%= f.label :summary %>
    <%= f.text_area :summary%>
    <%= f.collection_check_boxes :user_ids, User.all, :id, :first_name %>
    <%= f.submit %>
<%end%>
