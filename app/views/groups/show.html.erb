<h2>Group: <%= @group.title %>
<br>
<% @group.users.each do |u| %>
<div class="user_card">
  <h1><%= link_to "#{u.first_name} #{u.last_name}", user_path(u)%></h1>
    <p>Email: <%= u.email %></p>
    <p>Address: <%= u.address%></p>
    <p>Phone Number: <%= u.phone_number%></p>
  <img src="<%= u.image %>">
</div>
<% end %>
    <% if @current_user.admin %>
    <%= link_to 'Add Users', new_group_assignment_path%>
    <%end%>
