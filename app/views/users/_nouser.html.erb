  <%= link_to('Sign up', '/users/new') %>
  <br>
  <%= link_to('Log in with Facebook!', '/auth/facebook') %>

  <%= form_for(:session, url: login_path) do |f| %>

  <div class="field">
    <%= f.label :email %>:
    <%= f.text_field :email %>
  </div>

  <div class="field">
  <%= f.label :password %>:
  <%= f.password_field :password %>

  <div class="actions">
    <%= f.submit "Sign In"  %>
  </div>
  <% end %>